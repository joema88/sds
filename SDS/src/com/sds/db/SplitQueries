1. Raw data comparison
SELECT STOCKID,DATEID, PERCENT, CLOSE,NETCHANGE, ATR ,OPEN, HIGH, LOW ,LOW52 ,HIGH52 ,MARKCAP, VOLUME FROM BBROCK WHERE STOCKID=5 OR STOCKID=10005 ORDER BY DATEID DESC limit 60;
2.continue
SELECT STOCKID,DATEID, CLOSE,YELLOW ,TEAL,PINK,SC5,YP10,BT9,TSC5,DAYS,PTVAL,PTCP,PASS,CX520,CCX,BDCX, BDW,PTCP2,DAY2,ABT9,APAS,
 APTV FROM BBROCK WHERE STOCKID=5 OR STOCKID=4487 ORDER BY DATEID DESC limit 60;
 3.
 SELECT STOCKID,DATEID,CLOSE, BPY ,SYPT,TSM,TOM,PSM,POM,YSM,YOM, NSM, NOM,DSM,MOR,YOR ,TRK  FROM BBROCK WHERE STOCKID=1 ORDER BY DATEID DESC limit 60;
 
 4. SELECT STOCKID,DATEID,CLOSE, DM,DA ,ADM,RK,FUC,UPC,UDS,DPC,DDS FROM BBROCK WHERE STOCKID=5 OR STOCKID=10005 ORDER BY DATEID DESC limit 80;