1. Super bull query
//CCX 1 to 20, Pass Points sum >5, the bigger the better. 
//SUM of last 2 or 3 of PTVAL, PTVAL2 has some reference value, SUM PTVAL may be more important
select a.DATEID,CDATE, a.STOCKID,  DAYS,PTCP, PTVAL,PASS,CLOSE,CCX, PTCP2,DAY2,CX520,BDCX,BT9,BDW,b.SYMBOL FROM BBROCK a, SYMBOLS b,DATES c  WHERE a.STOCKID = b.STOCKID and a.DATEID=c.DATEID and ( b.SYMBOL='WST')  order by a.DATEID DeSC limit 2600;
//aslo observed that perfect BT9 bull is hard to come by
//we could relax the condition 10 out 9 Teal bar considered as a BT9 bull around AVG5>AVG20 junction
//i.e. CCX 1 to 20 or CCX -10 to -1 range, For example FF, MNST would qualify
//and make sense

2. Daily calculation comparison query (CIEN & CIEN2) to see error.

A. BASIC COMPARISION
SELECT A.STOCKID, DATEID, PERCENT, CLOSE,NETCHANGE,ATR,OPEN,HIGH,LOW,LOW52,HIGH52,MARKCAP,VOLUME,YELLOW,TEAL,PINK FROM BBROCK A, SYMBOLS B WHERE A.STOCKID = B.STOCKID AND (B.SYMBOL='CIEN' OR B.SYMBOL='CIEN2') ORDER BY A.DATEID DESC LIMIT 30;

B. CALCULATION DAILY VS. HISTORY COMPARISON
SELECT DATEID, CLOSE,YELLOW,TEAL,PINK,SC5,YP10,BT9,TSC5,DAYS,PTVAL,PTCP,PASS,CX520,CCX,BDCX,BDW,PTCP2,DAY2, B.SYMBOL FROM BBROCK A, SYMBOLS B WHERE A.STOCKID = B.STOCKID AND (B.SYMBOL='CIEN' OR B.SYMBOL='CIEN2') ORDER BY A.DATEID DESC LIMIT 30;

3. BULL PASS WITH CX520
SELECT SYMBOL,COUNT(*) FROM BBROCK A, SYMBOLS B WHERE A.STOCKID=B.STOCKID AND PASS>3 AND CCX>0 AND CCX<=14 GROUP BY SYMBOL ORDER BY COUNT(*) DESC;


4. SUPER BULL SIGNAL BY STOCK NAME
SELECT C.CDATE, CLOSE,YELLOW,TEAL,PINK,SC5,YP10,BT9,TSC5,DAYS,PTVAL,PTCP,PASS,CX520,CCX,BDCX,BDW,PTCP2,DAY2, B.SYMBOL FROM BBROCK A, SYMBOLS B, DATES C WHERE A.STOCKID = B.STOCKID  AND A.DATEID = C.DATEID AND PASS>3 AND CCX>0 AND CCX<=14 AND (B.SYMBOL='AMZN') ORDER BY A.DATEID DESC LIMIT 300;

SELECT A.DATEID, YELLOW,TEAL,PINK,SC5,TSC5,DAYS,YP10,BT9,C.CDATE, PTVAL,PTCP,PASS,CLOSE,CCX,PTCP2,DAY2, CX520,BDCX,BDW,B.SYMBOL FROM BBROCK A, SYMBOLS B, DATES C WHERE A.STOCKID = B.STOCKID  AND A.DATEID = C.DATEID AND (B.SYMBOL='NBR') ORDER BY A.DATEID DESC LIMIT 300;

5. SUPER BULL STATISTICS QUERY
SELECT SYMBOL,COUNT(*) FROM BBROCK A, SYMBOLS B WHERE A.STOCKID = B.STOCKID AND PASS>3 AND CCX>0 AND CCX<=14 GROUP BY SYMBOL ORDER BY COUNT(*) DESC;

SELECT COUNT(*) FROM BBROCK WHERE PASS>3 AND CCX>0 AND CCX<=14;


6. Latest BT9 query
select count(*) from BBROCK WHERE BT9 =8 and YP10 = 0 and DATEID = 8930;
select b.SYMBOL, MARKCAP, PERCENT from BBROCK a, SYMBOLS b WHERE a.STOCKID = b.STOCKID and BT9 =8 and YP10 = 0 and DATEID = 8930 ORDER BY PERCENT, MARKCAP DESC;

7. Regular query
select a.DATEID,CDATE, a.STOCKID, CLOSE, ATR,TEAL, YELLOW, PINK, SC5,BT9,TSC5,DAYS,PTVAL, PTCP, PASS, CX520,CCX,VOLUME, b.SYMBOL FROM BBROCK a, SYMBOLS b, DATES c  WHERE a.STOCKID = b.STOCKID and a.DATEID=c.DATEID and b.SYMBOL='MAR' order by a.DATEID DESC limit 300;
//check CX520 count
select b.SYMBOL, a.DATEID,CDATE, a.STOCKID, CLOSE, ATR,TEAL, YELLOW, PINK, SC5, BT9, TSC5, DAYS, PTVAL, PTCP, PASS, CX520, CCX, VOLUME, b.SYMBOL FROM BBROCK a, SYMBOLS b, DATES c  WHERE a.STOCKID = b.STOCKID and a.DATEID=c.DATEID and a.DATEID =8930 and CX520=1;

//stock with 10 year history count
select COUNT(*) FROM BBROCK WHERE DATEID=8900;

8. Continuous teal query
select a.STOCKID,SYMBOL,CLOSE, PERCENT, MARKCAP FROM BBROCK a, SYMBOLS b WHERE a.STOCKID = b.STOCKID and BT9=9 and YP10=0 and DATEID=8931 ORDER BY MARKCAP DESC;


9. Double Pass Query BT9 pass and ABT9 pass
stockdb8
10. CCX, PASS, APAS, BT9, ABT9 Query
//QUAILIFIED STOCKS
select a.STOCKID,SYMBOL,CLOSE, PERCENT, MARKCAP FROM BBROCK a, SYMBOLS b WHERE MARKCAP>0 and (PASS>=1 OR APAS>=1) AND a.STOCKID = b.STOCKID and DATEID=8932 and CCX<14 ORDER BY MARKCAP DESC;
//EACH STOCK
select a.DATEID,a.STOCKID, TEAL, DAYS,PTCP, PTVAL,BT9,PASS,CCX, CDATE, CLOSE,APAS,ABT9,APTV, PTCP2,DAY2,CX520,BDCX, BDW,b.SYMBOL FROM BBROCK a, SYMBOLS b,DATES c  WHERE a.STOCKID = b.STOCKID and a.DATEID=c.DATEID and ( b.SYMBOL='YRD')  order by a.DATEID DeSC limit 260;

11. BUG QUERY
SELECT * FROM BBROCK WHERE BT9=10  AND PTVAL>0.1;
//ONTX 8933 RECORD REMOVED FROM HISTORY CAL, SO MAYBE SHOULD NOT DELETE ALL RECORDS
select a.DATEID,a.STOCKID, TEAL, DAYS,PTCP, PTVAL,BT9,PASS,CCX, CDATE, CLOSE,APAS,ABT9,APTV, PTCP2,DAY2,CX520,BDCX, BDW,b.SYMBOL FROM BBROCK a, SYMBOLS b,DATES c  WHERE a.STOCKID = b.STOCKID and a.DATEID=c.DATEID and ( b.SYMBOL='ONTX')  order by a.DATEID DeSC limit 26;

//SYPT Whole stock market query
SELECT SYPT,DATEID FROM BBROCK WHERE STOCKID=1 AND DATEID>8922 ORDER BY DATEID DESC;

SELECT SYPT,a.DATEID, b.CDATE FROM BBROCK a, DATES b WHERE a.DATEID=b.DATEID AND STOCKID=1 AND a.DATEID>8522 ORDER BY DATEID DESC;


12. SUPER DUPER BULL QUERY WITH PASS, APAS, CCX considered for 10 days' sum
select SYMBOL, SUM(PASS),SUM(CCX), SUM(APAS) FROM BBROCK A, SYMBOLS B  WHERE A.STOCKID=B.STOCKID AND A.DATEID<=8935 AND A.DATEID>=8926  GROUP BY SYMBOL ORDER BY SUM(PASS)+SUM(APAS) DESC LIMIT 20 ;

13. SELECT * FROM BBROCK WHERE DATEID = 8958 AND ATR<-999;

SELECT * FROM BBROCK WHERE DATEID = 8958 AND CX520<-999;

select a.DATEID,a.STOCKID, CDATE, CX520,b.SYMBOL, ATR,CX520, MARKCAP FROM BBROCK a, SYMBOLS b,DATES c  WHERE a.STOCKID = b.STOCKID and a.DATEID=c.DATEID and ATR<-999 and a.DATEID =8958;

select a.DATEID,a.STOCKID, CDATE, b.SYMBOL, ATR, CX520, MARKCAP FROM BBROCK a, SYMBOLS b,DATES c  WHERE a.STOCKID = b.STOCKID and a.DATEID=c.DATEID and ( b.SYMBOL='ACI')  order by a.DATEID DeSC limit 260;


14.DOUBLE BULLS EACH DAY
//BUY 3, 6 MONTH OR 1 YEAR CALL AND PUT AT THE SAME TIME 10 TO 15% PRICE COST
 select a.DATEID,a.STOCKID, TEAL, DAYS,PTCP, PTVAL,BT9,PASS,CCX, CDATE, CLOSE,APAS,ABT9,APTV, PTCP2,DAY2,CX520,BDCX, BDW,b.SYMBOL FROM BBROCK a, SYMBOLS b,DATES c  WHERE a.STOCKID = b.STOCKID and a.DATEID=c.DATEID and a.DATEID = 8875
and ( PASS>=1 AND APAS>=1 );

15. TREND SPOTTER ... LONG TERM TREND
select a.DATEID,a.STOCKID, TEAL, DAYS,PTCP, PTVAL,BT9,PASS,CCX, CDATE, CLOSE,APAS,ABT9,APTV, PTCP2,DAY2,CX520,BDCX, BDW,b.SYMBOL FROM BBROCK a, SYMBOLS b,DATES c  WHERE a.STOCKID = b.STOCKID and a.DATEID=c.DATEID 
and BT9>=18 and a.DATEID = 8961;

16. TOP stocks of the day with COLOR indicators
//1 year overall, 1 month momental
SELECT a.DATEID,a.STOCKID, b.SYMBOL,CDATE,CLOSE, (TSM-YSM-2*PSM)/DSM, (TOM-YOM-2*POM)/25 FROM BBROCK a, SYMBOLS b,DATES c  WHERE a.STOCKID = b.STOCKID and a.DATEID=c.DATEID  AND a.DATEID=8595 and DSM=141 ORDER BY (TOM-YOM-2*POM)/25 DESC, (TSM-YSM-2*PSM)/DSM DESC limit 100;


SELECT a.DATEID,a.STOCKID, b.SYMBOL,CDATE,CLOSE,UPC,UDS,DPC,DDS, TSM,NSM,YSM,PSM,YOM,POM,TOM,(TSM-YSM-2*PSM)/DSM, (TOM-YOM-2*POM)/25  FROM BBROCK a, SYMBOLS b,DATES c  WHERE a.STOCKID = b.STOCKID and a.DATEID=c.DATEID  AND b.SYMBOL='GE' ORDER BY DATEID DESC LIMIT 200;

//color ranking
SELECT a.DATEID,a.STOCKID, b.SYMBOL,CDATE,CLOSE, MOR,YOR, TRK FROM BBROCK a, SYMBOLS b,DATES c  WHERE a.STOCKID = b.STOCKID and a.DATEID=c.DATEID  AND a.DATEID=8880 and DSM=426 and (MOR>0.0000000001 OR MOR<-0.00000001)  and TRK>0.001 ORDER BY MOR ASC, YOR ASC limit 100;

//classic stories
select a.DATEID,a.STOCKID, CDATE, b.SYMBOL, CLOSE, MOR, YOR, TRK, PASS, APAS, BT9,TSM,YSM,PSM,DSM FROM BBROCK a, SYMBOLS b,DATES c  WHERE a.STOCKID = b.STOCKID and a.DATEID=c.DATEID and ( b.SYMBOL='TSLA')  order by a.DATEID DeSC limit 550;

select a.DATEID,a.STOCKID, CDATE, b.SYMBOL, CLOSE, MOR, YOR, TRK, PASS, APAS, BT9,TSM,YSM,PSM,DSM FROM BBROCK a, SYMBOLS b,DATES c  WHERE a.STOCKID = b.STOCKID and a.DATEID=c.DATEID and ( b.SYMBOL='TWLO')  order by a.DATEID DeSC limit 550;


select a.DATEID,a.STOCKID, CDATE, b.SYMBOL, CLOSE, MOR, YOR, TRK, PASS, APAS, BT9,TSM,YSM,PSM,DSM FROM BBROCK a, SYMBOLS b,DATES c  WHERE a.STOCKID = b.STOCKID and a.DATEID=c.DATEID and ( b.SYMBOL='MRNA')  order by a.DATEID DeSC limit 550;

//FUC Query
SELECT a.DATEID, a.STOCKID, CDATE, b.SYMBOL, CLOSE, MOR, YOR, TRK, BT9, DPC, DDS, UDS, UPC, DM, DA, FUC
FROM BBROCK a, SYMBOLS b, DATES c WHERE a.STOCKID=b.STOCKID and a.DATEID = c.DATEID and ( b.SYMBOL='TSLA' and a.FUC>3)
order by a.DATEID DESC limit 3000;

//
select a.DATEID,a.STOCKID, CDATE, b.SYMBOL, CLOSE, MOR, YOR, TRK, PASS, APAS,DPC, DDS, UDS, UPC, DM, DA, FUC FROM BBROCK a, SYMBOLS b,DATES c  WHERE a.STOCKID = b.STOCKID and a.DATEID=c.DATEID and ( b.SYMBOL='AAPL')  order by a.DATEID DeSC limit 550;

//FUC OF THE DAY
select a.DATEID,a.STOCKID, CDATE, b.SYMBOL, CLOSE, MOR, YOR, TRK, PASS, APAS, BT9,TSM,YSM,PSM,DSM FROM BBROCK a, SYMBOLS b,DATES c  WHERE a.STOCKID = b.STOCKID and a.DATEID=c.DATEID and ( a.DATEID=9006)  AND a.FUC>3;

select a.DATEID,a.STOCKID, CDATE, b.SYMBOL, CLOSE, MOR, YOR, TRK, PASS, APAS,DPC, DDS, UDS, UPC, DM, DA, FUC FROM BBROCK a, SYMBOLS b,DATES c  WHERE a.STOCKID = b.STOCKID and a.DATEID=c.DATEID and   a.DATEID=9007 AND a.FUC>4;

select a.DATEID,a.STOCKID, CDATE, b.SYMBOL, MARKCAP,CLOSE, MOR, YOR, TRK, PASS, APAS,FLOOR(DPC), FLOOR(UPC), FLOOR(DM), FUC AS UTURN FROM BBROCK a, SYMBOLS b,DATES c  WHERE a.STOCKID = b.STOCKID and a.DATEID=c.DATEID and   a.DATEID=9008 AND a.FUC>4 ORDER BY MARKCAP DESC;


