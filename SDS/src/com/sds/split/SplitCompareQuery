STOCKID,DATEID,PERCENT,CLOSE,NETCHANGE,ATR,OPEN,HIGH,LOW,LOW52,HIGH52,MARKCAP,VOLUME,YELLOW,TEAL,PINK,CX520,

SC5,YP10,BT9,TSC5,DAYS,PTVAL,PTCP,PASS,CCX,BDCX,BDW,PTCP2,DAY2,ABT9,APAS,APTV,SYPT,BPY,TSM,TOM,PSM,

POM,YSM,YOM,NSM,NOM,DSM,MOR,YOR,TRK,

UPC,UDS,DPC,DDS,DM,DA,ADM,RK,FUC,DBY,PDY,IAY,IPY,SAY,SPY

--match okay
SELECT STOCKID,DATEID,PERCENT,CLOSE,NETCHANGE,ATR,OPEN,HIGH,LOW,LOW52,HIGH52,MARKCAP,VOLUME,YELLOW,TEAL,PINK,CX520 from BBROCK WHERE STOCKID=4651 or STOCKID=24651 ORDER BY DATEID DESC LIMIT 400;

SELECT STOCKID,DATEID,SC5,YP10,BT9,TSC5,DAYS,PTVAL,PTCP,PASS,CCX,BDCX,BDW,PTCP2,DAY2,ABT9,APAS,APTV,SYPT,BPY,TSM,TOM,PSM  from BBROCK WHERE STOCKID=4651 or STOCKID=24651 ORDER BY DATEID DESC LIMIT 400;

--match okay
SELECT STOCKID,DATEID,POM,YSM,YOM,NSM,NOM,DSM,MOR,YOR,TRK from BBROCK WHERE STOCKID=4651 or STOCKID=24651 ORDER BY DATEID DESC LIMIT 400;

SELECT STOCKID,DATEID,CLOSE,UPC,UDS,DPC,DDS,DM,DA,ADM,RK,FUC,BDY,PDY,IAY,IPY,SAY,SPY from BBROCK  WHERE  STOCKID=4651 or STOCKID=24651 ORDER BY DATEID DESC LIMIT 400;

